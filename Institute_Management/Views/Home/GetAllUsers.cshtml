@model IEnumerable<Institute_Management.Models.ApplicationUser>;

<table class="table table-light table-striped mt-5">

    <thead>
        <tr>
            <th>User Id</th>
            <th>Name</th>
            <th>User Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Institute</th>
            <th>Role</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>@item.PhoneNumber</td>
                <td>@item.InstituteName</td>
                <td>@item.UserRole</td>
                <partial name="_PartialButtonView" model="item.Id" />
            </tr>
        }
        <tr>
            
        </tr>
    </tbody>

</table>
